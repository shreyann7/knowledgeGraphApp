<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title><%= title %></title>
  <link rel="stylesheet" href="/stylesheets/style.css">
</head>
<body>
  <main>
    <h1><%= title %></h1>
    
    <% if (typeof messages !== 'undefined') { %>
      <% messages.forEach(function(msg) { %>
        <p><%= msg %></p>
      <% }); %>
    <% } %>
